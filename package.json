    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-tsparticles": "^2.9.3",
    "tailwindcss": "^3.2.7",
    "tinacms": "^2.2.4",
    "tsparticles": "^2.9.3",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
